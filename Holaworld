                </div>
            </div>
 
            <!--------------------------------------->
 
            <!-- who answered? -->
            {block:Answerer}
            <div class="reblog-wrap une_reponse">
                <div class="answerer reblog-head">
                    <img src="{AnswererPortraitURL-64}">
